{
  "name": "fimidara-server-node",
  "version": "0.1.0",
  "description": "fimidara file storage service server Node.js",
  "author": {
    "email": "ywordk@gmail.com",
    "name": "softkave"
  },
  "license": "MIT",
  "keywords": [],
  "type": "module",
  "engines": {
    "node": ">=13"
  },
  "scripts": {
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "npm run clean && tsc",
    "fix": "gts fix",
    "start": "node build/src/index.js",
    "pretest": "tsc",
    "prod": "tsc && env-cmd -f \".env.prod\" npm start",
    "dev": "tsc && env-cmd -f \".env.dev\" npm start",
    "dev:managed": "npx -y -- tsx src/tools/dev/index.ts dev",
    "test": "env-cmd -f \".env.unit-test\" vitest --config ./vitest.config.ts",
    "test:fn": "env-cmd -f \".env.unit-test\" vitest --config ./vitest.fn.config.ts",
    "test:integration": "env-cmd -f \".env.integration-test\" vitest --config ./vitest.config.ts",
    "dev:test": "tsc && env-cmd -f \".env.unit-test\" npm start",
    "check:circular-deps": "npx dpdm --no-warning --no-tree ./src/index.ts",
    "mongo:dev": "npx run-rs --host localhost --portStart 27000 --dbpath ./local-mongo/dev --keep",
    "mongo:test": "npx run-rs --host localhost --portStart 27005 --dbpath ./local-mongo/test",
    "render:email-templates": "env-cmd -f \".env.dev\" npx tsx src/tools/email-templates/index.ts",
    "gen:public:endpoints-info": "mfdoc gen-http-api-endpoints-info ./src/endpoints/endpoints.mfdoc.ts --output-path ../fimidara-mfdoc-out/public-endpoints --tags public",
    "gen:public:setup-js-sdk": "mfdoc setup-js-sdk ../fimidara-js-sdk --name fimidara --provider pnpm",
    "gen:public:js-sdk": "mfdoc gen-js-sdk ./src/endpoints/endpoints.mfdoc.ts --output-path ../fimidara-js-sdk --tags public --filename-prefix public",
    "gen:private:endpoints-info": "mfdoc gen-http-api-endpoints-info ./src/endpoints/endpoints.mfdoc.ts --output-path ../fimidara-mfdoc-out/private-endpoints --tags private",
    "gen:private:setup-js-sdk": "mfdoc setup-js-sdk ../fimidara-private-js-sdk --name fimidara-private --provider pnpm",
    "gen:private:js-sdk": "mfdoc gen-js-sdk ./src/endpoints/endpoints.mfdoc.ts --output-path ../fimidara-private-js-sdk --tags private --filename-prefix private",
    "setup:sdk-test:public": "npx env-cmd -f \".env.unit-test\" npx tsx src/tools/sdk-test-setup/index.ts",
    "setup:sdk-test:private": "npx env-cmd -f \".env.unit-test\" npx tsx src/tools/sdk-test-setup/index.ts ../fimidara-private-js-sdk/.env.test"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.705.0",
    "@aws-sdk/client-secrets-manager": "^3.465.0",
    "@aws-sdk/client-sesv2": "^3.52.0",
    "@aws-sdk/lib-storage": "^3.705.0",
    "@sesamecare-oss/redlock": "^1.3.1",
    "argon2": "^0.40.1",
    "busboy": "^1.6.0",
    "commander": "^14.0.0",
    "config": "^3.3.12",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
    "express": "^4.17.3",
    "express-jwt": "^8.3.0",
    "fs-extra": "^11.1.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "joi": "^17.9.2",
    "js-accessor": "^1.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash-es": "^4.17.21",
    "mfdoc": "^0.4.0",
    "mongoose": "8.15.1",
    "nanoid": "^5.0.7",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.1",
    "resend": "^6.1.2",
    "safe-stable-stringify": "^2.3.1",
    "sharp": "^0.32.6",
    "softkave-js-utils": "0.14.0",
    "softkave-node-utils": "^0.4.1",
    "stripe": "^8.222.0",
    "tsyringe": "^4.8.0",
    "type-fest": "^4.39.1",
    "validator": "^13.5.2",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "zx": "^7.2.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/busboy": "^1.5.4",
    "@types/config": "^3.3.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.9",
    "@types/faker": "^5.5.9",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash-es": "^4.17.12",
    "@types/multer": "^1.4.5",
    "@types/node": "^22.10.2",
    "@types/sharp": "^0.26.1",
    "@types/validator": "^13.1.2",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "concurrently": "^9.1.2",
    "env-cmd": "^10.1.0",
    "expect-type": "^0.19.0",
    "gts": "^5.3.0",
    "inquirer": "^9.2.15",
    "tsx": "^4.20.3",
    "typescript": "^5.3.3",
    "vitest": "^3.1.1"
  }
}
